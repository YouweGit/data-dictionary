{
    "name": "youwe/data-dictionary",
    "description": "A data dictionary for Pimcore",
    "type": "pimcore-bundle",
    "require": {
        "pimcore/pimcore": "^5.6 || ^6.2",
        "graphp/graphviz": "^0.2.1",
        "clue/graph": "^0.9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "license": "gpl-3.0",
    "autoload": {
        "psr-4": {
            "DataDictionaryBundle\\": ".\/src"
        }
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "DataDictionaryBundle\\DataDictionaryBundle"
            ]
        }
    },
    "authors": [
        {
            "name": "Paulo Vitor Bettini de Paiva Lima",
            "email": "p.bettini@youwe.nl"
        }
    ],
    "suggest": {
        "youwe/data-dictionary-object-bridge": "If you are using the youwe/PimcoreObjectBridge, you can use this package to show it on the data model"
    }
}
